{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "lint": "eslint src/**/*.ts src/**/*.tsx --no-error-on-unmatched-pattern",
    "test": "jest --detectOpenHandles --passWithNoTests",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test-watch": "npm run test --watch",
    "pre-apkbuild": "expo export --dev --public-url http://127.0.0.1:8000 && npx http-server -p 8000 dist &",
    "post-apkbuild": "pkill -f http-server",
    "dev-build-android": "EXPO_ANDROID_KEYSTORE_PASSWORD=safezone371 EXPO_ANDROID_KEY_PASSWORD=safezone371 turtle build:android --type apk --keystore-path ./keystore.jks --keystore-alias 'keyalias' --allow-non-https-public-url --public-url http://127.0.0.1:8000/android-index.json"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.0.7",
    "@react-navigation/native": "^6.0.4",
    "@types/node": "*",
    "expo": "^43.0.3",
    "expo-auth-session": "~3.3.1",
    "expo-constants": "~11.0.1",
    "expo-random": "~11.2.0",
    "expo-status-bar": "~1.0.4",
    "jest-expo": "^42.1.0",
    "jsonfile": "^6.1.0",
    "mibs": "file:../../lib/mibs/ts",
    "react": "^17.0.2",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.4.0",
    "react-native-unimodules": "^0.14.10",
    "react-native-web": "^0.17.5",
    "turtle-cli": "^0.23.5"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.7",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^27.0.2",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolve": "^0.0.10",
    "eslint": "^8.0.1",
    "eslint-config-google": "^0.14.0",
    "expo-cli": "^4.12.1",
    "jest-expo": "^42.1.0",
    "react-test-renderer": "^17.0.2",
    "ts-node": "^10.3.0",
    "typescript": "~4.0.0"
  },
  "private": true,
  "jest": {
    "verbose": true,
    "preset": "jest-expo",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  }
}
