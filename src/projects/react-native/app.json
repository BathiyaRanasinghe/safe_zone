{
  "expo": {
    "name": "react-native",
    "slug": "react-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "safe-zone",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "usask.cmpt371.team1project",
      "buildNumber": "1.0.0",
      "publishBundlePath": "./bundles/ios"
    },
    "android": {
      "package": "usask.cmpt371.team1project",
      "versionCode": 1,
      "publishBundlePath": "./bundles/android",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
